{
  "name": "compre",
  "description": "compares products for you",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "./images/Letter-C-icon.png",
    "48": "./images/Letter-C-icon.png",
    "128": "./images/Letter-C-icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.amazon.in/*","https://github.com/himanish-star"],
      "js": ["injectToDummyPage.js"]
    }
  ],
  "permissions":[
    "activeTab",
    "storage",
    "declarativeContent",
    "contextMenus"],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
